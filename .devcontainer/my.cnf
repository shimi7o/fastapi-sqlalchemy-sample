[mysqld]
init-connect='SET NAMES utf8mb4;'
character-set-server=utf8mb4
sql_mode=NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION,ONLY_FULL_GROUP_BY,STRICT_ALL_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_ZERO_DATE,NO_ZERO_IN_DATE
collation-server=utf8mb4_bin
default-time-zone='Asia/Tokyo'

[client]
default-character-set=utf8mb4
